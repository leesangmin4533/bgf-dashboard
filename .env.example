# BGF 리테일 로그인 정보 (점포별)
# 호반베르디움점 (46513)
BGF_USER_ID_46513=your_user_id
BGF_PASSWORD_46513=your_password

# 이천동양점 (46704)
BGF_USER_ID_46704=your_user_id
BGF_PASSWORD_46704=your_password

# 새 점포 추가 시 (예시)
# BGF_USER_ID_99999=your_user_id
# BGF_PASSWORD_99999=your_password

# 카카오 REST API 키 (developers.kakao.com → 내 애플리케이션 → 앱 키)
KAKAO_REST_API_KEY=your_rest_api_key
# 카카오 클라이언트 시크릿 (REST API 키 수정 → 클라이언트 시크릿 → 코드)
KAKAO_CLIENT_SECRET=your_client_secret

# 카카오 로그인 정보 (토큰 자동 재인증용, 선택사항)
KAKAO_ID=your_kakao_email_or_phone
KAKAO_PW=your_kakao_password
